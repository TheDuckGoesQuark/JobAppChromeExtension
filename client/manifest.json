{
  "manifest_version": 3,
  "name": "Job Application Chrome Extension",
  "short_name": "JAE",
  "description": "A quick way to fill out job application fields.",
  "version": "0.0.1",
  "author": "github.com/TheDuckGoesQuark",
  "action": {
    "default_title": "JAE",
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "./src/background/background.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./src/contentscripts/index.ts"],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "identity",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "*://localhost:*/*"
  ],
  "key": "",
  "oauth2": {
    "client_id": "1029116563390-ifde5d3c671gbuimic2ue3ri4uit1uam.apps.googleusercontent.com",
    "scopes": [
      "profile",
      "email",
      "openid"
    ]
  }
}